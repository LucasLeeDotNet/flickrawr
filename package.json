{
  "name": "flickrawr",
  "version": "1.0.0",
  "description": "Flickr, but louder, Rawr!",
  "main": "index.component.tsx",
  "scripts": {
    "build": "suitcss ./src/index.css public/style.css --minify && webpack --mode production",
    "deploy": "gh-pages -d build",
    "start": "webpack-dev-server --env.ENVIRONMENT=development --content-base build/ --mode development --watch --watch-poll",
    "suitup": "suitcss ./src/index.css build/style.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/LucasLee/flickrawr.git"
  },
  "keywords": [
    "flickr",
    "photo",
    "gallery",
    "rawr"
  ],
  "author": "Lucas Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/LucasLee/flickrawr/issues"
  },
  "homepage": "https://gitlab.com/LucasLee/flickrawr#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "stylelint": "^10.0.1",
    "stylelint-suitcss": "^3.0.0",
    "suitcss": "^3.0.0",
    "suitcss-preprocessor": "^4.0.0",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
